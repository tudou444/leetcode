//使用辅助数组，空间换时间，时间O（n）空间O（n）
class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int n = nums.size();
        int* temp = new int[n]{}; //temp[i]存储数字i+1的出现次数
        for(int i = 0; i < n; i++){
            if(nums[i] > 0 && nums[i] <= n){
                temp[nums[i]-1]++;
            }
        }
        for(int i = 0; i < n; i++){
            if(temp[i] == 0){
                return i+1;
            }
        }
        return n+1;
    }
};
